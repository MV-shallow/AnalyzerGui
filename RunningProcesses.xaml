<Page x:Class="AnalyzerGui.RunningProcesses"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AnalyzerGui"
      mc:Ignorable="d"
      Title="RunningProcesses" Loaded="WindowLoaded">

    <Grid Background="White">
        <ListView x:Name="ProcList" ScrollViewer.CanContentScroll="False">
            <ListView.View>
                <GridView AllowsColumnReorder="False">
                    <GridViewColumn Width="265" Header="Процесс"  DisplayMemberBinding="{Binding Path=ProcessName}"/>
                </GridView>
            </ListView.View>
        </ListView>

    </Grid>
</Page>
